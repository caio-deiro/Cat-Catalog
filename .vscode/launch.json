    "version": "0.2.0",
    "configurations": [
        {
            "name": "cat_list development",
            "request": "launch",
            "flutterMode": "debug",
            "type": "dart",
            "args": ["--dart-define-from-file", ".env", "--flavor", "development", "--target", "lib/main_dev.dart"] 
        },
        {
            "name": "cat_list staging",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "args": ["--flavor", "staging", "--target", "lib/main_stg.dart"]
        },
        {
            "name": "cat_list production",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "args": ["--flavor", "production", "--target", "lib/main.dart"]
        }
    ]
}